{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Permisos por defecto para la ventana principal",
  "windows": ["main"],
  "permissions": [
    "fs:allow-document-read-recursive",
    "fs:allow-document-write-recursive",
    "fs:allow-mkdir",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-read-file",
    "fs:allow-remove",
    "fs:allow-rename",

    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-execute",
    "log:allow-log"
  ]
}
